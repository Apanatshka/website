<html>
<head>
    <title>Blog | jam1garner</title>
    <link rel="shortcut icon" type="image/x-icon" href="/img/jam_transparent_side.png">
    <link rel="stylesheet" href="/css/global.css" media="all">
    <link rel="stylesheet" href="/css/index.css" media="all">
    <link rel="stylesheet" href="/css/sticky_sidebar.css" media="all">
    <meta property="og:title" content="Blog Home" />
    <meta property="og:type" content="website" />
    <meta property="og:site_name" content="jam1garner" />
    <meta property="og:description"content="Writeups, Hacking, and Hacky Software">
</head>
<body>
    {{> sticky}}
    <article>
        <h1 class="title">Posts</h1>
        {{#each posts}}
            <a class="post-card" href="/blog/{{name}}">
                <h2 class="card-title">{{title}}</h2>
                <img class="post-image" src="{{thumbnail}}" alt="">
                <div class="post-date">Posted: {{date}}</div>
            </a>
        {{/each}}
    </article>
</body>
</html>
